	</bean>
	<bean id="hzycb_JobTrigger" class="org.springframework.scheduling.quartz.CronTriggerBean">
		<property name="jobDetail" ref="hzycb_JobDetail"></property>
		<!-- 3分钟处理一次 -->
		<property name="cronExpression" value="0 0/20 * * * ?"></property>
